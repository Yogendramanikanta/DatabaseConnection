package com.DatabaseProject.service.impl;

import org.apache.naming.java.javaURLContextFactory;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.jdbc.core.JdbcTemplate;

import com.DatabaseProject.service.DataBaseservice;

public class DataBaseServiceImpl implements DataBaseservice {

	@Autowired
	JdbcTemplate template;
	@Override
	public String featchTheScemesList() {
		String Query = "SELECT schema_name\r\n"
				+ "FROM information_schema.schemata where schema_name not in ('information_schema','pg_toast','pg_catalog') ;\r\n"
				+ "";
		List<String> listOfServicves = template.execute(Query);
		
		return null;
	}
	
	

}
